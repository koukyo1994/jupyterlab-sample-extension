{
  "name": "jupyterlab-apod",
  "version": "0.1.0",
  "description": "Show a random NASA Astronomy Picture of the Day in a JupyterLab panel",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/koukyo1994/jupyterlab-sample-extension",
  "bugs": {
    "url": "https://github.com/koukyo1994/jupyterlab-sample-extension/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Hidehisa Arai",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/koukyo1994/jupyterlab-sample-extension.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib && rimraf tsconfig.tsbuildinfo",
    "prepare": "npm run clean && npm run build",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^1.1.0",
    "@jupyterlab/apputils": "^1.1.0",
    "@phosphor/messaging": "^1.3.0",
    "@phosphor/widgets": "^1.9.0"
  },
  "devDependencies": {
    "rimraf": "^2.6.1",
    "typescript": "~3.5.2"
  },
  "sideEffects": [
    "style/*.css"
  ],
  "jupyterlab": {
    "extension": true
  }
}
